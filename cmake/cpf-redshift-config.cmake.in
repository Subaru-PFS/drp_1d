@PACKAGE_INIT@ 

set_and_check ( cpf-redshift_INCLUDE_DIR "@cpf-redshift_INCLUDE_DIR@")
set_and_check ( cpf-redshift_THIRDPARTY_INCLUDE_DIR "@cpf-redshift_THIRDPARTY_INCLUDE_DIR@")

set_and_check ( cpf-redshift_LINK_DIR "@cpf-redshift_LINK_DIR@")
set_and_check ( cpf-redshift_THIRDPARTY_LINK_DIR "@cpf-redshift_THIRDPARTY_LINK_DIR@")
if ( ${CMAKE_BUILD_TYPE} STREQUAL "Release" )
  if ( BUILD_SHARED_LIBS )
    set_and_check ( cpf-redshift_LIB "@cpf-redshift_LINK_DIR@libcpf-redshift.so" )
    set ( cpf-redshift_THIRDPARTY_LIBS "@cpf-redshift_THIRDPARTY_LIBS@" )
  else()
    set_and_check ( cpf-redshift_LIB "@cpf-redshift_LINK_DIR@libcpf-redshift.a" )
    set ( cpf-redshift_THIRDPARTY_LIBS "" )
  endif()
endif()

if ( ${CMAKE_BUILD_TYPE} STREQUAL "Debug" )
  if ( BUILD_SHARED_LIBS )
    set_and_check ( cpf-redshift_LIBDEBUG "@cpf-redshift_LINK_DIR@libcpf-redshift-debug.so" )
    set ( cpf-redshift_THIRDPARTY_LIBS "@cpf-redshift_THIRDPARTY_LIBS@" )
  else()
    set_and_check ( cpf-redshift_LIB "@cpf-redshift_LINK_DIR@libcpf-redshift-debug.a" )
    set ( cpf-redshift_THIRDPARTY_LIBS "" )
  endif()
endif()

